{
  "name": "hacker-dashboard",
  "version": "1.2.0",
  "description": "Dashboard for participants, judges, mentors and partners of QHacks 2018.",
  "author": "Robert Saunders and Zack Harley",
  "scripts": {
    "server-dev": "cross-env NODE_ENV=development nodemon server",
    "server-debug": "cross-env NODE_ENV=development nodemon --inspect server",
    "start": "cross-env NODE_ENV=production node server",
    "client-dev": "cross-env webpack-dev-server",
    "client-prod": "cross-env npm run client-clean && NODE_ENV=production webpack",
    "client-clean": "cross-env rimraf ./client/bundle",
    "test": "cross-env eslint server/**/*.js client/**/*.js && nsp check --reporter checkstyle && ncu -e 2 -x mongoose",
    "email-dev": "cross-env npm run email-clean && mjml ./email/templates/*.mjml -o ./email/dist/",
    "email-prod": "cross-env npm run email-clean && mjml --min ./email/templates/*.mjml -o ./email/dist/",
    "email-clean": "cross-env rimraf ./email/dist && mkdir ./email/dist"
  },
  "dependencies": {
    "@google-cloud/error-reporting": "^0.2.2",
    "@sendgrid/mail": "^6.1.4",
    "axios": "^0.17.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "camelcase": "^4.1.0",
    "classnames": "^2.2.5",
    "compression": "^1.7.1",
    "connect-history-api-fallback": "^1.5.0",
    "cross-env": "^5.1.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "helmet": "^3.9.0",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "mjml": "^3.3.5",
    "moment": "^2.19.2",
    "mongoose": "^4.13.9",
    "mustache": "^2.3.0",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.1",
    "react": "^16.1.1",
    "react-confetti": "^2.0.2",
    "react-dom": "^16.1.1",
    "react-emoji-render": "^0.4.3",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-form": "^7.1.2",
    "redux-form-validators": "^2.1.2",
    "redux-saga": "^0.16.0",
    "semantic-ui-less": "^2.2.12",
    "semantic-ui-react": "^0.77.2",
    "uuid": "^3.1.0",
    "validator": "^9.1.2",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "compression-webpack-plugin": "^1.0.1",
    "css-loader": "^0.28.8",
    "eslint": "^4.15.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "nodemon": "^1.14.11",
    "npm-check-updates": "^2.13.0",
    "nsp": "^3.1.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-dev-server": "^2.10.1"
  }
}
