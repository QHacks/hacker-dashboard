{
  "name": "hacker-dashboard",
  "version": "0.0.0",
  "description": "Dashboard for participants, judges, mentors and partners of QHacks 2018.",
  "author": "Robert Saunders, Zack Harley, Cory Crowley & Avi Ihejirika",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server",
    "start": "cross-env NODE_ENV=production node server",
    "ui": "cross-env webpack-dev-server",
    "build": "cross-env NODE_ENV=production npm run clean && webpack -p",
    "test": "cross-env eslint server/**.js client/**.js && nsp check --reporter checkstyle && ncu -e 2",
    "deploy": "cross-env gcloud app deploy",
    "clean": "cross-env rimraf ./client/bundle"
  },
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "cross-env": "^5.1.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.1",
    "express-jwt": "^5.3.0",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "moment": "^2.19.1",
    "mongoose": "^4.13.1",
    "react": "^16.1.0",
    "react-dom": "^16.1.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "redux": "^3.7.2",
    "redux-form": "^7.1.2",
    "validator": "^9.1.1",
    "semantic-ui-less": "^2.2.12",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.9.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "npm-check-updates": "^2.13.0",
    "nsp": "^3.1.0",
    "rimraf": "^2.6.2",
    "sass-lint": "^1.12.1",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  }
}
