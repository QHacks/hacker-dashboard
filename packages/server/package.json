{
  "name": "server",
  "version": "2.1.0",
  "description": "The server for the QHacks Dashboard!",
  "main": "index.js",
  "scripts": {
    "dev": "npm run build && NODE_ENV=development nodemon --ext graphql,js,json index.js",
    "start": "NODE_ENV=production node index.js",
    "build": "npm run clean && mjml --min ./emails/templates/*.mjml -o ./emails/dist/",
    "build-emails": "npm run build",
    "clean": "rimraf ./emails/dist && mkdir ./emails/dist"
  },
  "author": "QHacks Engineering",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^6.3.1",
    "apollo-server-express": "^2.2.2",
    "axios": "^0.18.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "connect-history-api-fallback": "^1.5.0",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "graphql": "^14.0.2",
    "graphql-resolvers": "^0.2.2",
    "graphql-tools": "^4.0.3",
    "helmet": "^3.13.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "mjml": "^4.1.2",
    "mustache": "^2.3.2",
    "pg": "^7.6.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.41.2",
    "sequelize-cli": "^5.3.0",
    "uuid": "^3.3.2",
    "validator": "^10.7.1",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "apollo-server-testing": "^2.2.6",
    "jest": "^23.6.0",
    "jest-environment-node": "^23.4.0",
    "nodemon": "^1.18.4",
    "rimraf": "^2.6.2",
    "supertest": "^3.3.0"
  }
}
