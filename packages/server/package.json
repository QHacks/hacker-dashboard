{
  "name": "server",
  "version": "1.3.4",
  "description": "The server for the QHacks Dashboard!",
  "main": "index.js",
  "scripts": {
    "dev": "npm run build && NODE_ENV=development nodemon index.js",
    "start": "NODE_ENV=production node index.js",
    "build": "npm run clean && mjml --min ./emails/templates/*.mjml -o ./emails/dist/",
