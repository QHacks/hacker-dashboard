    "axios": "^0.18.0",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "camelcase": "^5.0.0",
    "compression": "^1.7.3",
    "connect-history-api-fallback": "^1.5.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "helmet": "^3.13.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "mjml": "^4.1.2",
    "moment": "^2.22.2",
    "mongoose": "^5.2.14",
    "mustache": "^2.3.2",
    "uuid": "^3.3.2",
    "validator": "^10.7.1",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "jest-environment-node": "^23.4.0",
    "mongodb-memory-server": "^2.2.0",
    "nodemon": "^1.18.4",
    "rimraf": "^2.6.2",
    "supertest": "^3.3.0"
  }
